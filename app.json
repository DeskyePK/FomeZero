{
  "expo": {
    "name": "FomeZero",
    "slug": "FomeZero",
    "scheme": "fomezero",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Logo.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "intentFilters": [
      ],
      "package": "com.anonymous.FomeZero"
    },
    "permissions": [
      "android.permission.INTERNET"
    ],
    "adaptiveIcon": {
      "foregroundImage": "./assets/adaptive-icon.png",
      "backgroundColor": "#ffffff"
    },
    "package": "com.deskye.FomeZero",
    "config": {
      "cleartext": {
        "cleartextTrafficPermitted": true
      }
    }
  },
  "web": {
    "favicon": "./assets/favicon.png"
  },
  "extra": {}
}
